<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.taqtile.takitiletest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".domain.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".domain.activities.LoginSuccessActivity" />
        <activity android:name=".domain.activities.UserListActivity" />
        <activity android:name=".domain.activities.UserDetailsActivity2" />
        <activity android:name=".domain.activities.CreateNewUserActivity" />
        <activity android:name=".domain.activities.EditUserActivity" />
        <activity android:name=".presentation.userDetails.UserDetailsActivity"></activity>
    </application>

</manifest>